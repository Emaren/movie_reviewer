<%= simple_form_for [@movie, @movie.reviews.build] do |f| %>
<div id="rating-form">
  <label>Rating</label>
</div>

  <%= f.input :comment %>
  <%= f.button :submit %>
  <% end %>

  <script>
    $('#rating-form').raty({
      path: '/assets/',
      scoreName: 'review[rating]'
    });
    </script>
